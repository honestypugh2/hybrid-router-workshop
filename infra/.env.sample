# Azure Infrastructure Environment Variables
# Copy this file to .env and update with your values

# Required: APIM Admin Email (must be set)
APIM_ADMIN_EMAIL=your-email@domain.com

# Optional: Azure Configuration (defaults provided)
AZURE_LOCATION=eastus2
WORKLOAD_NAME=hybridllm
ENVIRONMENT_NAME=dev

# Optional: Service Configuration
OPENAI_SKU_NAME=S0
APIM_SKU_NAME=Developer
APIM_ORGANIZATION_NAME=Hybrid LLM Router

# Optional: Feature Toggles
ENABLE_TELEMETRY=true
ENABLE_AI_FOUNDRY=true

# Optional: Azure Identity (auto-populated by azd)
AZURE_PRINCIPAL_ID=your-user-id