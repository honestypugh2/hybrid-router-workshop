{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workloadName": {
      "value": "${WORKLOAD_NAME=hybridllm}"
    },
    "environmentName": {
      "value": "${ENVIRONMENT_NAME=dev}"
    },
    "location": {
      "value": "${AZURE_LOCATION=eastus2}"
    },
    "enableTelemetry": {
      "value": "${ENABLE_TELEMETRY=true}"
    },
    "enableAiFoundry": {
      "value": "${ENABLE_AI_FOUNDRY=true}"
    },
    "openAiSkuName": {
      "value": "${OPENAI_SKU_NAME=S0}"
    },
    "apimSkuName": {
      "value": "${APIM_SKU_NAME=Developer}"
    },
    "apimAdminEmail": {
      "value": "${APIM_ADMIN_EMAIL}"
    },
    "apimOrganizationName": {
      "value": "${APIM_ORGANIZATION_NAME=Hybrid LLM Router}"
    },
    "tags": {
      "value": {
        "project": "hybrid-router-workshop",
        "owner": "${AZURE_PRINCIPAL_ID=azd-user}",
        "environment": "${ENVIRONMENT_NAME=dev}"
      }
    }
  }
}